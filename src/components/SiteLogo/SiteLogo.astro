---
import { Image } from "astro:assets";

// data
import siteData from "@config/siteData.json";

// images
import logo from "@images/site-logo.png";
---

<a class="primary-focus flex items-center align-middle" href="/" class="">
  <Image
    src={logo}
    alt="Dalyle DevOps Inc."
    loading="eager"
    width={100}
    height={100}
    class="h-14 w-14"
    aria-hidden="true"
  />
  <div class="text-2xl">{siteData.name}</div>
</a>
